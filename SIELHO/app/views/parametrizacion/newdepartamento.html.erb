		<h2>Departamento </h2>
        <%semantic_form_for @departamento do |f| %>
			<% f.inputs do %>
				<%= f.input  :nombre%>
				<%= f.input :descripcion, :input_html => { :style => "width:70em; height: 10em" }%>
			<% end %>
			<% f.buttons do %>
				<%= f.commit_button :label =>"Crear"%>
			<% end %>
		<% end %>
 
